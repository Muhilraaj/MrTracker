{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "$BACKEND_URL/api/*"
    },
    {
      "route": "/apt/*",
      "rewrite": "$BACKEND_URL/apt/*"
    },
    {
      "route": "/auth/*",
      "rewrite": "$BACKEND_URL/auth/*"
    }
  ],
   "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/auth/*"]
  }
}
